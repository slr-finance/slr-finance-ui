    "@vueuse/core": "8.9.4",
    "@walletconnect/web3-provider": "1.7.7",
    "bignumber.js": "9.0.2",
    "dayjs": "1.11.0",
    "ethers": "5.5.2",
    "lightweight-charts": "3.8.0",
